{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "class-methods-use-this": "off",
        "no-unused-vars": "off",
        "no-undef": "off",
        "no-underscore-dangle": "off",
        "no-tabs": "off",
        "max-len": "off",
        "no-param-reassign": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
